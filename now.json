{
  "name": "zeit-chat",
  "version": 2,
  "alias": ["my-domain.com", "my-alias"],
  "scope": "my-team",
   "build": {
    "env": {
      "MY_KEY": "this is the value",
      "SECRET": "@my-secret-name"
    }
  },
  "builds": [
    { "src": "*.html", "use": "@now/static" },
    { "src": "*.php", "use": "@now/php" },
    { "src": "*.js", "use": "@now/node" }
  ],
  "public": true,
  
}
